<Window x:Class="NHIRD.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NHIRD"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="165.833" Loaded="Window_Loaded" ResizeMode="NoResize">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button HorizontalAlignment="Center" VerticalAlignment="Center" Click="Button_Click" Margin="0,10" Width="75">Get _CD</Button>
            <Button HorizontalAlignment="Center" VerticalAlignment="Center" Click="ButtonGetOrder_Click" Margin="0,10" Width="75">Get _Order</Button>
            <Button HorizontalAlignment="Center" VerticalAlignment="Center" Click="ButtonID_Click" Margin="0,10" Width="75">ID檔標準化</Button>
            <Button HorizontalAlignment="Center" VerticalAlignment="Center" Click="ButtonIDSplit_Click" Margin="0,10" Width="75">ID Split by Hash</Button>
        </StackPanel>
    </Grid>
</Window>
